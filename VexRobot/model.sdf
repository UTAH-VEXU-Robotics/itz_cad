<?xml version="1.0"?>
<sdf version="1.5">
  <model name="VEX Robot">

    <link name='chassis'>
       <pose>0 0 .15 0 0 3.14159</pose>
       <inertial>
        <inertia>
          <ixx>0.3</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.5</iyy>
          <iyz>0</iyz>
          <izz>0.3</izz>
        </inertia>
        <mass>10.0</mass>
      </inertial>
      <collision name='collision'>
        <geometry>
          <mesh><uri>file://Collisions/ChassisCollision.dae</uri></mesh>
        </geometry>
      </collision>

      <visual name='visual'>
        <geometry>
          <mesh><uri>file://VexRobot/chassis.dae</uri></mesh>
        </geometry>
      </visual>
    </link>

